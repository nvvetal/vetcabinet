<!DOCTYPE html>
<!--[if lt IE 7]>
<html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>
<html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>
<html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js"> <!--<![endif]-->
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="apple-mobile-web-app-capable" content="yes"/>
    <title>{% block title %}VetCabinet{% endblock %}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    {% include 'VetCabinetFrontendBundle::i_facebook_meta.html.twig' %}
    {% block meta %}{% endblock %}
    {% block stylesheets %}
        {% stylesheets
        '@VetCabinetAsseticBundle/Resources/public/css/styles.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="screen">
        {% endstylesheets %}
    {% endblock %}
    {% block javascripts %}
        {% javascripts
        'http://code.jquery.com/jquery-2.1.0.min.js'
        '@VetCabinetAsseticBundle/Resources/public/js/main.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
</head>
<body>
    {% include 'VetCabinetFrontendBundle::header.html.twig' %}
    {% include 'VetCabinetFrontendBundle::content.html.twig' %}
    {% include 'VetCabinetFrontendBundle::footer.html.twig' %}
<div id="fb-root"></div>
<script>
    (function() {
        var e = document.createElement('script'); e.async = true;
        e.src = document.location.protocol +
                '//connect.facebook.net/ru_RU/all.js';
        document.getElementById('fb-root').appendChild(e);
    }());
</script>
</body>
</html>